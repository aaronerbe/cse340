<header id="top-header">
    <span class="siteName">
        <a href="/" title="Return to home page">CSE Motors</a>
    </span>
    <div id="tools">
        <!--<a title="Click to log in" href="/account/login">My Account</a>     -->
        <!-- value that will be sent through reqest object to app:  "/account/login".  expects back a login view.  Need an "accounts" router file (similar to inventoryRoute.js) to handle the request -->

        <% if (loggedin) { %>
            <a title="Inventory Management" href="/account/">Welcome <%- fName %></a>
            <br>
            <a title="Click to logout" href="/account/logout">Logout</a>
        <% } else { %>
            <a title="Click to log in" href="/account/login">My Account</a>
        <% } %>
    </div>
</header>